{
  "dashboard": {
    "id": null,
    "title": "Seata Server Dashboard",
    "tags": ["seata", "distributed-transactions"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Transaction Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(seata_transaction_total[5m])",
            "legendFormat": "Transactions/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Branch Execution Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(seata_branch_success_total[5m]) / (rate(seata_branch_success_total[5m]) + rate(seata_branch_failure_total[5m])) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Branch Execution Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(seata_branch_latency_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(seata_branch_latency_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(seata_branch_latency_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Active Transactions",
        "type": "graph",
        "targets": [
          {
            "expr": "seata_active_transactions",
            "legendFormat": "Active Transactions"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Branch Execution Count",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(seata_branch_success_total[5m])",
            "legendFormat": "Successful Branches/sec"
          },
          {
            "expr": "rate(seata_branch_failure_total[5m])",
            "legendFormat": "Failed Branches/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
